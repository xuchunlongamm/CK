#include <stdio.h>
int main() {
    char str[100];
    scanf("%[^\n]s", str);getchar();
    printf("has %d digits\n", printf("%s", str));
    scanf("%[^\n]s", str);
    printf("has %d digits\n", printf("%s", str));
    return 0;
}
